{
  "expo": {
    "name": "WorkBreak",
    "slug": "WorkBreak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "workbreak",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/Designer.png",
        "backgroundImage": "./assets/images/Designer.png",
        "monochromeImage": "./assets/images/Designer.png",
        "dark": {
          "backgroundColor": "#E6F4FE",
          "foregroundImage": "./assets/images/Designer.png",
          "backgroundImage": "./assets/images/Designer.png",
          "monochromeImage": "./assets/images/Designer.png"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.WorkBreak"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Designer.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#E6F4FE",
          "dark": {
            "image": "./assets/images/Designer.png",
            "backgroundColor": "#E6F4FE"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/Designer.png",
          "color": "#4CAF50",
          "sounds": [
            "./assets/sounds/bell.wav",
            "./assets/sounds/chime.mp3",
            "./assets/sounds/alert.mp3",
            "./assets/sounds/notification.wav",
            "./assets/sounds/ringtone.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
